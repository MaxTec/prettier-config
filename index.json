{
  "$schema": "http://json.schemastore.org/prettierrc",
  "trailingComma": "none",
  "endOfLine": "lf",
  "printWidth": 100,
  "semi": true,
  "tabWidth": 2,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "jsxBracketSameLine": true,
  "importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
